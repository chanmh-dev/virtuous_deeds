<script>
	$(document).ready(function() {
			$('#full-prostration-counter').text(2000);
			$('#half-prostration-counter').text(1000);
			$('.card-counter').each(function () {
			$(this).prop('Counter',0).animate({
			Counter: $(this).text()
			}, {
					duration: 4000,
					easing: 'swing',
					step: function (now) {
					$(this).text(Math.ceil(now));
					}
				});
			});
	});
</script>

